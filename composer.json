{
    "name": "joaojacome/clogger-php",
    "description": "A logger preprocessor",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joao Jacome",
            "email": "joao@joaojaco.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Clogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
